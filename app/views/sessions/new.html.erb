<h1>Log In</h1>
<br>
<form class="new-session" action="<%= session_url %>" method="POST">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <label for="email">Email Address</label>
  <input id="email" type="text" name="email" value="<%= params[:email] %>">
  <br>
  <label for="password">Password</label>
  <input id="password" type="password" name="password">
  <br>
  <button class="new-session-button">Submit</button>
</form>
<br>
<a class="sign-up-link" href="<%= new_user_url %>">Sign Up</a>
