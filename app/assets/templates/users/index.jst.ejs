<h1>Add Collaborators</h1>

<% users.forEach(function (user) { %>
  <li class="show-user">
    <%= user.escape("fname") %> <%= user.escape("lname") %> <%= user.id %>
    <% if (collaborators.get(user.id)) { %>
      <button class="remove-collaborator">Remove</button>
    <% } else {%>
      <button class="add-collaborator">Add</button>
    <% } %>

    </li>
<% }) %>
