
<br>
<p>Projects</p>
 <% projects.forEach(function(project) { %>
   <li class="project-tile">
     <h2 class="project-title"><a href="#projects/<%= project.id %>"><%= project.escape("title") %></a></h2>
     <p class="project-description">
       <%= project.escape("description") %>
     </p>
     <span class="ownership-status">
       <% if (project.id === project.get("current_user_id")) { %>
         (owner)
       <% } else { %>
         (collaborator)
       <% } %>
     </span>
    </li>
 <% }) %>
<li class="new-project-tile">
  <h2><a href="#projects/new">Create New Project</a></h2>
  <p>
    Your journey begins here.
  </p>
</li>
